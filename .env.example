# Circle API Configuration (Production/Sandbox Mode)
VITE_CIRCLE_API_KEY=TEST_API_KEY:your_api_key:your_secret
VITE_CIRCLE_API_URL=https://api.circle.com
VITE_CIRCLE_WALLET_SET_ID=your_wallet_set_id
VITE_CIRCLE_ENTITY_SECRET_HEX=your_entity_secret_hex
VITE_CIRCLE_PUBLIC_KEY_PEM="-----BEGIN PUBLIC KEY-----
your_public_key_here
-----END PUBLIC KEY-----"

# Supabase Configuration
# Replace with your actual Supabase project settings
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
SUPABASE_DB_PASSWORD=your_database_password
SUPABASE_DB_URL=postgresql://postgres:password@host:5432/postgres
VITE_USE_LOCAL_SUPABASE=false

# Development Settings
NODE_ENV=development
VITE_APP_ENVIRONMENT=development

# Filecoin/Web3.Storage Configuration
VITE_FILECOIN_ENABLED=true
VITE_WEB3_STORAGE_DID=your_web3_storage_did_here
VITE_WEB3_STORAGE_PRIVATE_KEY=your_web3_storage_private_key_here
VITE_WEB3_STORAGE_SPACE_DID=your_web3_storage_space_did_here

# Tavus AI Configuration
VITE_TAVUS_API_KEY=your_tavus_api_key
VITE_TAVUS_PERSONA_ID=your_tavus_persona_id

# Circle API Configuration for Node.js scripts
CIRCLE_API_KEY=your_circle_api_key
CIRCLE_WALLET_ID=your_circle_wallet_id

# Algorand Blockchain Configuration
VITE_ALGORAND_NETWORK=testnet
VITE_ALGORAND_MNEMONIC=your_25_word_mnemonic_phrase_here